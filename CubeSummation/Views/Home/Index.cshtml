@model CubeSummation.Models.ExecuteTestCaseModel

@{
    ViewBag.Title = "Cube Summation";
}

<div class="row">
    <div class="col-md-12">
        <h3>Content</h3>
    </div>
    @using (Html.BeginForm("ExecuteTestCase", "Home", FormMethod.Post))
    {
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="col-md-12">
            @Html.TextAreaFor(m => m.Content)
            @Html.ValidationMessageFor(m => m.Content, "", new { @class = "text-danger" })
        </div>
        <button type="submit" class="btn btn-primary">Execute</button>
    }

</div>
